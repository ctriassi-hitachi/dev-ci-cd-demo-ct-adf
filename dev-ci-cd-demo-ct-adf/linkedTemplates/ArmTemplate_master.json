{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dev-ci-cd-demo-ct-adf"
		},
		"ls_devcicdctdemo_managed_identity_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devcicddemoctdl.dfs.core.windows.net/"
		},
		"WeatherAPILinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.openweathermap.org/data/2.5/"
		},
		"ls_weather_dl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devcicddemoctdl.dfs.core.windows.net/"
		},
		"ls_azure_key_vault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://dev-ci-cd-demo-ct-kv.vault.azure.net/"
		},
		"ls_cicddemodl_access_key_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devcicddemoctdl.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dev-ci-cd-demo-ct-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_devcicdctdemo_managed_identity_properties_typeProperties_url": {
						"value": "[parameters('ls_devcicdctdemo_managed_identity_properties_typeProperties_url')]"
					},
					"WeatherAPILinkedService_properties_typeProperties_url": {
						"value": "[parameters('WeatherAPILinkedService_properties_typeProperties_url')]"
					},
					"ls_weather_dl_properties_typeProperties_url": {
						"value": "[parameters('ls_weather_dl_properties_typeProperties_url')]"
					},
					"ls_azure_key_vault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_azure_key_vault_properties_typeProperties_baseUrl')]"
					},
					"ls_cicddemodl_access_key_properties_typeProperties_url": {
						"value": "[parameters('ls_cicddemodl_access_key_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}